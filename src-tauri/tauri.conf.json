{
	"build": {
		"beforeDevCommand": "pnpm dev",
		"beforeBuildCommand": "pnpm build",
		"frontendDist": "../dist",
		"devUrl": "http://localhost:1420"
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	},
	"productName": "meteor",
	"version": "0.0.4",
	"identifier": "com.meteor.dev",
	"plugins": {},
	"app": {
		"security": {
			"csp": null
		},
		"windows": [
			{
				"title": "meteor",
				"width": 800,
				"height": 600,
				"center": true
			}
		]
	}
}
